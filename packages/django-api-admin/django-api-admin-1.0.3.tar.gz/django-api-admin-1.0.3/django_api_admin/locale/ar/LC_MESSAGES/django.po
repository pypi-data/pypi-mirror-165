#: views.py:355
msgid "The {opts.verbose_name} “{str(new_object)}” was added successfully."
msgstr "تمت اضافة {opts.verbose_name} “{str(new_object)}” بنجاح."

#: views.py:408
msgid ""
"The {opts.verbose_name} “{str(updated_object)}” was changed successfully."
msgstr "تم تغير {opts.verbose_name} “{str(updated_object)}” بنجاح."

#: views.py:454
#, python-format
