{
  "name": "@epi2melabs/epi2melabs-wfpage",
  "version": "0.2.9",
  "description": "A jupyterlab launcher for Nextflow workflows",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/epi2me-labs/epi2melabs-wfpage",
  "bugs": {
    "url": "https://github.com/epi2me-labs/epi2melabs-wfpage/issues"
  },
  "license": "BSD-3-Clause",
  "author": "epi2melabs",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/epi2me-labs/epi2melabs-wfpage.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf epi2melabs_wfpage/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jlpm run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@jupyterlab/application": "^3.1.0",
    "@jupyterlab/coreutils": "^5.1.0",
    "@jupyterlab/docmanager": "^3.2.3",
    "@jupyterlab/filebrowser-extension": "^3.2.8",
    "@jupyterlab/launcher": "^3.2.4",
    "@jupyterlab/notebook": "^3.0.10",
    "@jupyterlab/services": "^6.1.0",
    "@jupyterlab/settingregistry": "^3.1.0",
    "ajv": "^8.8.0",
    "chonky": "^2.3.2",
    "chonky-icon-fontawesome": "^2.3.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react-markdown": "^8.0.3",
    "react-router-dom": "^6.3.0",
    "remark-gfm": "^3.0.1",
    "styled-components": "^5.3.5",
    "tsdef": "^0.0.14"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.1.0",
    "@types/lodash": "^4.14.182",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "epi2melabs_wfpage"
        }
      }
    },
    "extension": true,
    "outputDir": "epi2melabs_wfpage/labextension",
    "schemaDir": "schema",
    "_build": {
      "load": "static/remoteEntry.93c385138ea985e994be.js",
      "extension": "./extension",
      "style": "./style"
    }
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab~=3.1",
        "jlpm"
      ]
    }
  }
}
